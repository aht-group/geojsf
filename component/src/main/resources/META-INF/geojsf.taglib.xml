<facelet-taglib version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
        http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd">
	<namespace>http://geojsf.org/ui</namespace> 
	<composite-library-name>geojsf</composite-library-name> 
	<tag>
		<tag-name>map</tag-name>
		<component>
			<component-type>org.geojsf.component.Map</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>sldParameter</tag-name>
		<component>
			<component-type>org.geojsf.component.SldParameter</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>sld</tag-name>
		<component>
			<component-type>org.geojsf.component.Sld</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>scales</tag-name>
		<component>
			<component-type>org.geojsf.component.ScalesContainer</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>IosCheckbox</tag-name>
		<component>
			<component-type>org.geojsf.component.IosCheckbox</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>scaleBar</tag-name>
		<component>
			<component-type>org.geojsf.component.ScaleBar</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>time</tag-name>
		<component>
			<component-type>org.geojsf.component.Time</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>player</tag-name>
		<component>
			<component-type>org.geojsf.component.Player</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>timeControl</tag-name>
		<component>
			<component-type>org.geojsf.component.TimeControl</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>externalScript</tag-name>
		<component>
			<component-type>org.geojsf.component.UIExternalScript</component-type>
		</component>
	</tag>		
	<tag>
		<tag-name>coordinate</tag-name>
		<component>
			<component-type>org.geojsf.component.Coordinate</component-type>
		</component>
	</tag>		
	<tag>
		<tag-name>layer</tag-name>
		<component>
			<component-type>org.geojsf.component.Layer</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>layerSwitcher</tag-name>
		<component>
			<component-type>org.geojsf.component.LayerSwitcher</component-type>
		</component>
	</tag>	
	<tag>
		<tag-name>zoom</tag-name>
		<component>
			<component-type>org.geojsf.component.Zoom</component-type>
		</component>
	</tag>	
	<tag>
		<tag-name>viewport</tag-name>
		<component>
			<component-type>org.geojsf.component.Viewport</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>legend</tag-name>
		<component>
			<component-type>org.geojsf.component.Legend</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>sqlViewParameter</tag-name>
		<component>
			<component-type>org.geojsf.component.SqlViewParameter</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>viewParameter</tag-name>
		<component>
			<component-type>org.geojsf.component.ViewParameter</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>graticule</tag-name>
		<component>
			<component-type>org.geojsf.component.Graticule</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>rotation</tag-name>
		<component>
			<component-type>org.geojsf.component.Rotation</component-type>
		</component>
	</tag>
<!-- <tag>
		<tag-name>switchLayer</tag-name>
		<behavior>
			<behavior-id>org.geojsf.SwitchLayer</behavior-id>
		</behavior>
		<attribute>
		    <description>ID of the layer that is to be switched</description>
		    <name>layerId</name>
		    <required>true</required>
  		</attribute>
	</tag>	-->
</facelet-taglib>