<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://geojsf.sf.net/geojsf"
    xmlns:geojsf="http://geojsf.sf.net/geojsf"
    xmlns:ol="http://geojsf.sf.net/openlayers"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:import namespace="http://geojsf.sf.net/openlayers" schemaLocation="openlayers.xsd"/>
	<xsd:element name="query">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ol:repository" maxOccurs="1"/>
				<xsd:element ref="ol:service" maxOccurs="1"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>  
	<xsd:element name="coordinate">
        <xsd:complexType>
            <xsd:attribute name="lat" type="xsd:double"/>
            <xsd:attribute name="lon" type="xsd:double"/>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
