<facelet-taglib version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
        http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd">
	<namespace>http://geojsf.org/ui</namespace> 
	<composite-library-name>geojsf</composite-library-name> 
	<tag>
		<tag-name>map</tag-name>
		<component>
			<component-type>org.geojsf.component.Map</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>scale</tag-name>
		<component>
			<component-type>org.geojsf.component.Scale</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>time</tag-name>
		<component>
			<component-type>org.geojsf.component.Time</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>player</tag-name>
		<component>
			<component-type>org.geojsf.component.Player</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>externalScript</tag-name>
		<component>
			<component-type>org.geojsf.component.UIExternalScript</component-type>
		</component>
	</tag>		
	<tag>
		<tag-name>coordinate</tag-name>
		<component>
			<component-type>org.geojsf.component.Coordinate</component-type>
		</component>
	</tag>		
	<tag>
		<tag-name>layer</tag-name>
		<component>
			<component-type>org.geojsf.component.Layer</component-type>
		</component>
	</tag>
	<tag>
		<tag-name>layerSwitcher</tag-name>
		<component>
			<component-type>org.geojsf.component.LayerSwitcher</component-type>
		</component>
	</tag>	
	<tag>
		<tag-name>zoom</tag-name>
		<component>
			<component-type>org.geojsf.component.Zoom</component-type>
		</component>
	</tag>	
		<tag>
		<tag-name>viewport</tag-name>
		<component>
			<component-type>org.geojsf.component.Viewport</component-type>
		</component>
	</tag>	
</facelet-taglib>